    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-import-helpers": "^0.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4"
  }
